{
  "name": "fairtronics.org",
  "version": "1.0.0",
  "description": "Source files for generating the [fairtronics.org](https://fairtronics.org) website.",
  "main": ".eleventy.js",
  "scripts": {
    "start": "ELEVENTY_ENV=development npx eleventy --serve src",
    "build": "ELEVENTY_ENV=production npx eleventy src",
    "css-build": "node-sass --omit-source-map-url src/sass/styles.scss src/css/styles.css",
    "css-watch": "npm run css-build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/fairloetet/fairtronics.org.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/fairloetet/fairtronics.org/issues"
  },
  "homepage": "https://gitlab.com/fairloetet/fairtronics.org#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "bulma": "^0.8.0",
    "node-sass": "^4.13.1"
  },
  "dependencies": {
    "lottie-web": "^5.6.5"
  }
}
